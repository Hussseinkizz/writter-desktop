{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "dialog:default",
    "store:default",
    {
      "identifier": "fs:allow-app-read-recursive",
      "paths": [
        "*"
      ]
    },
    {
      "identifier": "fs:allow-app-meta-recursive",
      "paths": [
        "*"
      ]
    },
    "store:default",
    "store:default"
  ]
}